{
  "expo": {
    "name": "haulXpress",
    "slug": "haulXpress",
    "owner": "qwamicodes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/2a691fc6-637a-43f5-aa75-d9ac3607c078"
    },
    "splash": {
      "image": "./assets/app/splash.png",
      "backgroundColor": "#F9FAFB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qwamicodes.haulXpress",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "favicon": {
      "image": "./assets/app/adaptive-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app/adaptive-icon.png",
        "backgroundColor": "#F9FAFB"
      },
      "package": "com.qwamicodes.haulXpress"
    },
    "web": {
      "favicon": "./assets/app/adaptive-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a691fc6-637a-43f5-aa75-d9ac3607c078"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "HaulXpress app requires photo library access",
          "cameraPermission": "HaulXpress app requires camera access",
          "microphonePermission": false
        }
      ]
    ],
    "scheme": ["haulxpress"]
  }
}
