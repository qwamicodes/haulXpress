{
  "name": "haulxpress",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:local": "npx expo start",
    "start:cache": "npx expo start -c",
    "start:fast": "EXPO_USE_EXOTIC=1 expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "lint": "eslint src && yarn tsc",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "fix": "npx expo install --check",
    "publish": "eas update",
    "login": "eas login",
    "logout": "eas logout",
    "register": "eas register",
    "whoami": "eas whoami",
    "build:ios": "eas build:ios",
    "build:android": "eas build:android",
    "build": "yarn build:ios && yarn build:android",
    "prebuild": "npx expo prebuild",
    "build:preview": "eas build -p android --profile preview",
    "upload:ios": "expo upload:ios",
    "upload:android": "expo upload:android",
    "upload": "yarn upload:ios && yarn upload:android"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "expo": "^49.0.0",
    "expo-auth-session": "~5.0.2",
    "expo-image-picker": "~14.3.1",
    "expo-linking": "~5.0.2",
    "expo-random": "~13.2.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "^2.4.0",
    "expo-updates": "~0.18.10",
    "expo-web-browser": "~12.3.2",
    "firebase": "^10.0.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-fast-image": "^8.6.3",
    "react-native-geocoding": "^0.5.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-google-places-autocomplete": "^2.5.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.7.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/lodash": "^4.14.195",
    "@types/react": "~18.2.14",
    "@types/react-native-dotenv": "^0.2.0",
    "react-native-animatable": "^1.3.3",
    "react-native-dotenv": "^3.4.9",
    "typescript": "^5.1.3"
  },
  "private": true
}
