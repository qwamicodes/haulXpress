{
  "name": "haulxpress",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:local": "npx expo start",
    "start:cache": "npx expo start -c",
    "start:fast": "EXPO_USE_EXOTIC=1 expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "lint": "eslint src && yarn tsc",
    "test:debug": "jest -o --watch --coverage=false",
    "test:final": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "fix": "npx expo install --check",
    "doctor": "npx expo-doctor",
    "publish": "eas update",
    "login": "eas login",
    "logout": "eas logout",
    "register": "eas register",
    "whoami": "eas whoami",
    "build:ios": "eas build:ios",
    "build:android": "eas build:android",
    "build": "yarn build:ios && yarn build:android",
    "prebuild": "npx expo prebuild",
    "build:preview": "eas build -p android --profile preview",
    "upgrade-all-deb": "upgrade-interactive --latest",
    "upload:ios": "expo upload:ios",
    "upload:android": "expo upload:android",
    "upload": "yarn upload:ios && yarn upload:android"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.3",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/elements": "^1.3.30",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@react-navigation/stack": "^6.4.0",
    "@reduxjs/toolkit": "^2.2.6",
    "expo": "^51.0.18",
    "expo-auth-session": "^5.5.2",
    "expo-image-picker": "^15.0.7",
    "expo-linking": "^6.3.1",
    "expo-random": "^14.0.1",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "^1.12.1",
    "expo-system-ui": "^3.0.7",
    "expo-updates": "^0.25.18",
    "expo-web-browser": "^13.0.3",
    "firebase": "^10.12.3",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "^0.74.3",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-google-places-autocomplete": "^2.5.6",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.14.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "rn-placeholder": "^3.0.3",
    "yarn-upgrade-all": "^0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@types/lodash": "^4.17.6",
    "@types/react": "~18.2.79",
    "@types/react-native-dotenv": "^0.2.2",
    "react-native-animatable": "^1.4.0",
    "react-native-dotenv": "^3.4.11",
    "typescript": "~5.3.3"
  },
  "private": true
}
